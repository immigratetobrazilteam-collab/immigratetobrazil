{
  "name": "my-site",
  "version": "2.0.0",
  "private": true,
  "description": "Modern scalable redesign for Immigrate to Brazil",
  "scripts": {
    "dev": "WATCHPACK_POLLING=true CHOKIDAR_USEPOLLING=1 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit -p tsconfig.typecheck.json",
    "migrate:routes": "node scripts/generate-route-index.mjs",
    "cms:validate": "node scripts/validate-cms.mjs && node scripts/validate-state-guides.mjs && node scripts/validate-discover-pages.mjs && node scripts/validate-managed-legacy-pages.mjs",
    "state-guides:import": "node scripts/import-state-guides-from-blog.mjs",
    "state-guides:validate": "node scripts/validate-state-guides.mjs",
    "discover:import": "node scripts/import-discover-pages.mjs",
    "discover:validate": "node scripts/validate-discover-pages.mjs",
    "legacy:import": "node scripts/import-managed-legacy-pages.mjs",
    "legacy:validate": "node scripts/validate-managed-legacy-pages.mjs",
    "legacy:snapshot": "node scripts/materialize-legacy-snapshot.mjs",
    "content:reimport:from-legacy-ref": "npm run legacy:snapshot && LEGACY_SOURCE_ROOT=.legacy-snapshot npm run state-guides:import && LEGACY_SOURCE_ROOT=.legacy-snapshot npm run discover:import && LEGACY_SOURCE_ROOT=.legacy-snapshot npm run legacy:import && npm run cms:validate",
    "content:retention:audit": "node scripts/audit-content-retention.mjs",
    "content:coverage:report": "node scripts/content-coverage-report.mjs",
    "cms:sync-locales": "node scripts/sync-site-copy-locales.mjs --scope=managedPages --mode=copy",
    "cms:sync-locales:check": "node scripts/sync-site-copy-locales.mjs --scope=managedPages --mode=copy --check",
    "cms:sync-locales:translate": "node scripts/sync-site-copy-locales.mjs --scope=all --mode=translate",
    "content:check": "node scripts/content-publish-check.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "cms:backup": "node scripts/export-cms-backup.mjs",
    "seo:audit": "node scripts/seo-audit.mjs",
    "seo:env:check": "node scripts/check-seo-env.mjs",
    "seo:clusters": "node scripts/seo-clusters-ai.mjs",
    "seo:clusters:apply": "node scripts/seo-clusters-ai.mjs --apply",
    "seo:psi": "node scripts/pagespeed-insights.mjs",
    "seo:weekly:report": "node scripts/seo-weekly-report.mjs",
    "seo:autopilot": "node scripts/seo-clusters-ai.mjs --apply --days 90 && npm run cms:validate",
    "seo:final": "node scripts/seo-final-check.mjs",
    "env:validate": "node scripts/validate-env-integrations.mjs",
    "editorial:check": "node scripts/editorial-stale-check.mjs",
    "editorial:check:strict": "EDITORIAL_FAIL_ON_STALE=true node scripts/editorial-stale-check.mjs",
    "indexnow:key": "node scripts/indexnow.mjs key",
    "indexnow:submit": "node scripts/indexnow.mjs submit",
    "indexnow:submit:sitemap": "node scripts/indexnow.mjs submit --sitemap --limit=200",
    "dns:google:verify": "node scripts/ensure-google-verification-dns.mjs",
    "smoke": "node scripts/smoke-check.mjs",
    "perf:budget": "node scripts/perf-budget.mjs",
    "visual:smoke": "node scripts/visual-smoke.mjs",
    "production:readiness": "npm run env:validate && npm run cms:validate && npm run typecheck && npm run lint && npm test && npm run build && npm run perf:budget && npm run editorial:check",
    "verify:release": "node scripts/verify-release.mjs",
    "preview:worker": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf:typegen": "wrangler types"
  },
  "dependencies": {
    "@sentry/nextjs": "^10.40.0",
    "clsx": "^2.1.1",
    "next": "^15.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.16.5",
    "@playwright/test": "^1.58.2",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.2.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "vitest": "^4.0.18",
    "wrangler": "^4.67.0"
  }
}
